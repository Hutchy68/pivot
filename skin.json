{
	"name": "Pivot",
	"version": "2.1.0",
	"author": [
		"Tom Hutchison",
		"..."
	],
	"license-name": "BSD-2-Clause",
	"url": "http://github.com/hutchy68/pivot",
	"descriptionmsg": "pivot-desc",
	"type": "skin",
	"ValidSkinNames": {
		"pivot": "Pivot"
	},
	"MessagesDirs": {
